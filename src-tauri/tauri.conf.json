{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:30321",
    "distDir": "../dist"
  },
  "package": {
    "productName": "quickgletran",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }

    },
    "windows": [
      {
        "title": "QuickGletrans",
        "width": 900,
        "maxWidth": 900,
        "minWidth": 900,
        
        "height": 320,
        "maxHeight": 320,
        "minHeight": 320,
        
        "center": true,
        "fullscreen": false,
        "resizable": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "pltlabs",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
